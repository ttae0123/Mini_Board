{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    {{#article}}
        <tr>
            <th>{{id}}</th>
            <th>{{title}}</th>
            <td>{{content}}</td>
            <td>{{author}}</td>
            <td>{{createdAt}}</td>
        </tr>
    {{/article}}
</table>
